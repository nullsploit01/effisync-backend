{
  "name": "effisync-backend",
  "version": "0.0.0",
  "description": "Backend for EffiSync Project",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "start": "tsc && node ./dist/server.js",
    "dev": "ts-node-dev src/server.ts --respawn --transpileOnly",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --write \"./src/**/*{js,ts}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullsploit01/effisync-backend.git"
  },
  "author": "nullsploit01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nullsploit01/effisync-backend/issues"
  },
  "homepage": "https://github.com/nullsploit01/effisync-backend#readme",
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint \"./src/**/*{js,ts}\" --fix",
      "prettier --write \"./src/**/*{js,ts}\""
    ]
  }
}
